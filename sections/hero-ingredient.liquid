<section>
    {% if section.settings.heading != blank %}
        <h2>{{ section.settings.heading }}</h2>
    {% endif %}
</section>

{% schema %}
{
  "name": "Hero Ingredient",
  "tag":"section",
  "class":"section",
  "settings": [
    {
      "type": "text",
      "id": "heading",
      "label": "Heading",
      "default": "Hero Ingredients"
    }
  ],
  "blocks":[],
  "presets": [
    {
      "name": "Hero Ingredient"
    }
  ]
}
{% endschema %}